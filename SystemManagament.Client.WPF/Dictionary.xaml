    <system:String x:Key="Label_BIOS_BuildNumber">BuildNumber:</system:String>
    <system:String x:Key="Label_BIOS_BuildNumber_ToolTip">Internal identifier for this compilation of this software element.</system:String>
    <system:String x:Key="Label_BIOS_ReleaseDate">Release Date:</system:String>
    <system:String x:Key="Label_BIOS_ReleaseDate_ToolTip">Release date of the Windows BIOS in the Coordinated Universal Time (UTC).</system:String>
    <system:String x:Key="Label_BIOS_SerialNumber">Serial Number:</system:String>
    <system:String x:Key="Label_BIOS_SerialNumber_ToolTip">Assigned serial number of the software element.</system:String>
    <system:String x:Key="Label_BIOS_EmbeddedControllerMajorVersion">Embedded controller major version:</system:String>
