    <system:String xml:space="preserve"  x:Key="NetworkAdapter_InterfaceName">Interface&#x0a;Name</system:String>
    <system:String x:Key="NetworkAdapter_InterfaceName_ToolTip">The locally unique identifier for the network interface in InterfaceType_NetluidIndex format.</system:String>
    <system:String xml:space="preserve"  x:Key="NetworkAdapter_NdisMedium">Ndis&#x0a;Medium</system:String>
