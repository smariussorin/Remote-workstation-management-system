    <system:String x:Key="Label_BIOS_EmbeddedControllerMinorVersion">Embedded controller minor version:</system:String>
    <system:String x:Key="Label_BIOS_EmbeddedControllerMinorVersion_ToolTip">The minor release of the embedded controller firmware.</system:String>
    <system:String x:Key="Label_BIOS_SMBIOSBIOSVersion">SMBIOS BIOS Version:</system:String>
    <system:String x:Key="Label_BIOS_SMBIOSBIOSVersion_ToolTip">BIOS version as reported by SMBIOS.</system:String>
    <system:String x:Key="Label_BIOS_SMBIOSMajorVersion">SMBIOS major version:</system:String>
    <system:String x:Key="Label_BIOS_SMBIOSMajorVersion_ToolTip">Major SMBIOS version number.</system:String>
    <system:String x:Key="Label_BIOS_SMBIOSMinorVersion">SMBIOS minor version:</system:String>
    <system:String x:Key="Label_BIOS_SMBIOSMinorVersion_ToolTip">Minor SMBIOS version number.</system:String>
    <system:String x:Key="Label_BIOS_SMBIOSPresent">SMBIOS Present:</system:String>
    <system:String x:Key="Label_BIOS_SMBIOSPresent_ToolTip">If true, the SMBIOS is available on this computer system.</system:String>
    <system:String x:Key="Label_BIOS_SystemBiosMajorVersion">System bios major version:</system:String>
    <system:String x:Key="Label_BIOS_SystemBiosMajorVersion_ToolTip">The major release of the System BIOS.</system:String>
    <system:String x:Key="Label_BIOS_SystemBiosMinorVersion">System bios minor version:</system:String>
    <system:String x:Key="Label_BIOS_SystemBiosMinorVersion_ToolTip">The minor release of the System BIOS.</system:String>

